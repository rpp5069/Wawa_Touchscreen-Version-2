        <Button Name="pic3" HorizontalAlignment="Left" Margin="530,120,0,0" VerticalAlignment="Top" Width="119" Height="90" Background="White" BorderBrush="#FFF7F7FA">
            <Image Source="soup3.png" Stretch="UniformToFill"/>
        </Button>
        <Button Name="pic4" HorizontalAlignment="Left" Margin="130,261,0,0" VerticalAlignment="Top" Width="119" Height="90" Background="White" BorderBrush="#FFF7F7FA" Foreground="#FFF7F7FA">
            <Image Source="side1.png" Stretch="UniformToFill"/>
        </Button>
        <Button Name="pic5" HorizontalAlignment="Left" Margin="330,261,0,0" VerticalAlignment="Top" Width="119" Height="90" Background="#FFFFFCFC" BorderBrush="#FFF7F7FA" Foreground="#FFF7F7FA">
            <Image Source="side2.png" Stretch="UniformToFill"/>
        </Button>
        <Button Name="pic6" HorizontalAlignment="Left" Margin="530,261,0,0" VerticalAlignment="Top" Width="119" Height="90" Background="#FFFFFCFC" BorderBrush="#FFF7F7FA" Foreground="#FFF7F7FA">
            <Image Source="side3.png" Stretch="UniformToFill"/>
        </Button>
        <Label Content="Chicken Noodle" HorizontalAlignment="Left" Margin="144,209,0,0" VerticalAlignment="Top"/>
        <Label Content="Stuffing" HorizontalAlignment="Left" Margin="565,347,0,0" VerticalAlignment="Top"/>
        <Label Content="Chili" HorizontalAlignment="Left" Margin="366,347,0,0" VerticalAlignment="Top"/>
        <Label Content="Mashed Poatatoes" HorizontalAlignment="Left" Margin="136,347,0,0" VerticalAlignment="Top"/>
        <Label Content="Brocolli Cheddar" HorizontalAlignment="Left" Margin="538,209,0,0" VerticalAlignment="Top"/>
        <Label Content="Chicken Corn Chowder" HorizontalAlignment="Left" Margin="325,209,0,0" VerticalAlignment="Top"/>

    </Grid>
